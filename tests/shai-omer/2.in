// test branching in if-else statements

void main () {
    int x = 20;
    int y = 30;
    int z = x + y;
    int w = z / 10;

    if (x == 20) {
        print("GOOD");
    } 
    else {
        print("BAD");
    }

    if (y == 20) {
        print("BAD");
    } 
    else {
        print("GOOD");
    }

    if (z > x) {
        if (w * 20 <= z) {
            print("BAD");
        }
        else {
            print("GOOD");
        }
    }
    else {
        if (w * 20 <= z) {
            print("BAD");
        }
        else {
            print("BAD");
        }
    }

    if (z > x and w != 5) {
        if (w * 20 <= z or true) {
            print("BAD");
        }
        else {
            print("BAD");
        }
    }
    else {
        if (true and w * 20 <= z) {
            print("BAD");
        }
        else {
            print("GOOD");
        }
    }

}